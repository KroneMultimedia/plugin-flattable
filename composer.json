{
  "name": "wordpress/plugin-flattable",
  "type": "wordpress-plugin",
  "support": {
    "src": "http://gitlab.krone.at/WordPress/plugin-flattable"
  },
  "extra": {
    "installer-name": "kmm-flattable"
  },
  "repositories": [{
    "type": "gitlab",
    "url": "http://gitlab.krone.at/WordPress/mu-plugin-acf-extension.git"
  }],
  "require": {
    "php": ">=7.0.0",
    "wordpress/mu-plugin-acf-extension": "0.*",
    "composer/installers": "~1.0"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "config": {
    "gitlab-domains": ["gitlab.krone.at"],
    "github-protocols": ["https"],
    "secure-http": false
  },
  "require-dev": {
    "php-mock/php-mock": "^2.0"
  }
}
